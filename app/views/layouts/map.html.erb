    <script src="https://maps.googleapis.com/maps/api/js?sensor=true&libraries=drawing,places">  </script>

  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
  <%= render 'layouts/header' %> 

   <div id="map_canvas">
    <%= yield %>
    </div>
   <div id="sidebar">
          <table class="table table-bordered">
          	   <tr>
          	   	   <td><i  class="icon-globe icon-white"></i> <icon-glass> <%=  @situation_geosmap.name%></td>
          	   </tr>
          	</table>
      <script>displayGeoPanel()</script>      	
   </div>
    
</body>
</html>

